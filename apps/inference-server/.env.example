# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# Backend API Configuration
BACKEND_URL=http://localhost:8000
INTERNAL_API_TOKEN=test-integration-token

